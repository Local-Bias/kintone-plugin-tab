{
  "name": "kintone-plugin-tab",
  "version": "2.5.0",
  "author": "ribbit",
  "license": "MIT",
  "scripts": {
    "build": "ts-node scripts/marge-json.mjs prod && kbuild --mode plugin --env prod && kintone-plugin-packer --out dist/plugin-prod.zip --ppk dist/private.ppk plugin",
    "dev": "node scripts/marge-json.mjs dev && kintone-plugin-packer --out dist/plugin-dev.zip --ppk dist/private.ppk plugin && concurrently \"yarn:dev:*\"",
    "dev:js": "kbuild --mode plugin --env dev",
    "dev:upload": "node scripts/upload.mjs",
    "lint": "eslint ./src --cache --ext .js,.ts,.tsx"
  },
  "browser": {
    "path": false
  },
  "devDependencies": {
    "@emotion/css": "11.11.2",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@kintone/dts-gen": "7.0.4",
    "@kintone/plugin-packer": "7.0.4",
    "@kintone/plugin-uploader": "8.0.0",
    "@kintone/rest-api-client": "4.1.0",
    "@kintone/webpack-plugin-kintone-plugin": "7.0.4",
    "@konomi-app/kbuild": "^2.0.2",
    "@konomi-app/kintone-utilities": "^3.1.0",
    "@konomi-app/kintone-utility-component": "^1.2.0",
    "@lb-ribbit/kintone-xapp": "0.4.0",
    "@mui/icons-material": "5.14.7",
    "@mui/material": "5.14.7",
    "@sentry/react": "7.66.0",
    "@types/luxon": "3.3.2",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.6.0",
    "@typescript-eslint/parser": "6.6.0",
    "concurrently": "^8.2.1",
    "dotenv": "16.3.1",
    "eslint": "8.48.0",
    "immer": "10.0.2",
    "luxon": "3.4.2",
    "notistack": "3.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.11",
    "recoil": "0.7.7",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  }
}
