{
  "name": "kintone-plugin-tab",
  "version": "2.5.0",
  "author": "ribbit",
  "license": "MIT",
  "scripts": {
    "build": "ts-node scripts/marge-json.mjs prod && kbuild --mode plugin --env prod && kintone-plugin-packer --out dist/plugin-prod.zip --ppk dist/private.ppk plugin",
    "dev": "node scripts/marge-json.mjs dev && kintone-plugin-packer --out dist/plugin-dev.zip --ppk dist/private.ppk plugin && concurrently \"yarn:dev:*\"",
    "dev:js": "kbuild --mode plugin --env dev",
    "dev:upload": "node scripts/upload.mjs",
    "lint": "eslint ./src --cache --ext .js,.ts,.tsx"
  },
  "browser": {
    "path": false
  },
  "devDependencies": {
    "@emotion/css": "11.11.0",
    "@emotion/react": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@kintone/dts-gen": "6.1.30",
    "@kintone/plugin-packer": "6.0.36",
    "@kintone/plugin-uploader": "7.1.10",
    "@kintone/rest-api-client": "3.3.12",
    "@kintone/webpack-plugin-kintone-plugin": "6.0.36",
    "@konomi-app/kbuild": "^1.2.0",
    "@konomi-app/kintone-utilities": "^2.0.0",
    "@lb-ribbit/kintone-xapp": "0.4.0",
    "@mui/icons-material": "5.11.16",
    "@mui/material": "5.13.0",
    "@sentry/react": "7.51.2",
    "@types/luxon": "3.3.0",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@typescript-eslint/parser": "5.59.5",
    "concurrently": "^8.0.0",
    "dotenv": "16.0.3",
    "eslint": "8.40.0",
    "immer": "10.0.2",
    "luxon": "3.3.0",
    "notistack": "3.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.4",
    "recoil": "0.7.7",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  }
}
