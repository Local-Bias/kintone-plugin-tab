{
  "name": "kintone-plugin-tab",
  "version": "2.5.0",
  "author": "ribbit",
  "license": "MIT",
  "scripts": {
    "build": "ts-node scripts/marge-json.mjs prod && kbuild --mode plugin --env prod && kintone-plugin-packer --out dist/plugin-prod.zip --ppk dist/private.ppk plugin",
    "dev": "node scripts/marge-json.mjs dev && kintone-plugin-packer --out dist/plugin-dev.zip --ppk dist/private.ppk plugin && concurrently \"yarn:dev:*\"",
    "dev:js": "kbuild --mode plugin --env dev",
    "dev:upload": "node scripts/upload.mjs",
    "lint": "eslint ./src --cache --ext .js,.ts,.tsx"
  },
  "browser": {
    "path": false
  },
  "devDependencies": {
    "@emotion/css": "11.10.6",
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@kintone/dts-gen": "6.1.27",
    "@kintone/plugin-packer": "6.0.33",
    "@kintone/plugin-uploader": "7.1.10",
    "@kintone/rest-api-client": "3.3.9",
    "@kintone/webpack-plugin-kintone-plugin": "6.0.33",
    "@konomi-app/kbuild": "^1.2.0",
    "@konomi-app/kintone-utilities": "^1.4.0",
    "@lb-ribbit/kintone-xapp": "0.4.0",
    "@mui/icons-material": "5.11.16",
    "@mui/material": "5.12.0",
    "@sentry/react": "7.48.0",
    "@types/luxon": "3.3.0",
    "@types/react": "18.0.35",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "@typescript-eslint/parser": "5.57.0",
    "concurrently": "^8.0.0",
    "dotenv": "16.0.3",
    "eslint": "8.37.0",
    "immer": "9.0.21",
    "luxon": "3.3.0",
    "notistack": "3.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.3",
    "recoil": "0.7.7",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  }
}
